# Server Configuration
PORT=3462
NODE_ENV=development

# Database
DATABASE_URL="postgresql://username:password@localhost:5432/analytics_service_db"

# Redis
REDIS_URL="redis://localhost:6379"
REDIS_PASSWORD=""

# JWT Configuration
JWT_SECRET="your-super-secret-jwt-key-for-analytics-service"
JWT_EXPIRE_TIME="24h"

# CORS
ALLOWED_ORIGINS="http://localhost:3000,http://localhost:3001"

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=200

# Logging
LOG_LEVEL="info"
LOG_FORMAT="json"

# Service Discovery
USER_SERVICE_URL="http://localhost:3456"
INTERACTION_SERVICE_URL="http://localhost:3457"
QUEUING_SERVICE_URL="http://localhost:3458"
HISTORY_SERVICE_URL="http://localhost:3459"
COMMUNICATION_SERVICE_URL="http://localhost:3461"

# Data Aggregation Settings
METRICS_AGGREGATION_INTERVAL="0 0 * * *"
HOURLY_METRICS_INTERVAL="0 * * * *"
CLEANUP_OLD_METRICS_DAYS=90
BATCH_SIZE=1000

# Cache Settings
CACHE_TTL_SECONDS=300
DASHBOARD_CACHE_TTL=60

# Alert Settings
ALERT_CHECK_INTERVAL=300
WEBHOOK_URL=""
SLACK_WEBHOOK_URL=""

# External APIs
ANALYTICS_API_KEY=""
MONITORING_SERVICE_URL=""

# Performance Monitoring
ENABLE_PERFORMANCE_TRACKING=true
TRACK_QUERY_PERFORMANCE=true
SLOW_QUERY_THRESHOLD_MS=1000